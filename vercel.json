{
  "version": 2,
  "routes": [
    {
      "src": "/api/server/(.*)",
      "dest": "https://feetcode-production.up.railway.app/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://feetcode-production.up.railway.app"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}